{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "start:ts": "npm run type-check && run-p develop type-check:watch",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "gatsby": "2.1.37",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-typescript": "2.0.11",
    "gatsby-source-contentful": "2.0.39",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-remark": "^2.1.15",
    "node-sass": "^4.11.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "typescript": "3.3.3333",
    "yarn": "^1.12.3"
  },
  "devDependencies": {
    "gatsby-plugin-tslint": "^0.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0"
