{
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "dist",
        "target": "es5",
        "lib": ["es2017.intl", "es2017.object", "es2017.string", "es2016", "es2015", "dom", "esnext.asynciterable"],
        "jsx": "react",
        "module": "esnext",
        "sourceMap": true,
        "allowJs": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "noUnusedLocals": true
    }
}